{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exlude": [ 
            "node_modules/", 
            "mock.js", 
            "docs/", 
            "tests/" 
        ],
        "include": [ 
            "lib/"
        ]
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "footer": "Created by Karim Piyar Ali",
        "copyright": "2015"
    },
    "opts": {
        "destination": "docs",
        "recurse": true,
        "private": true
    }
}